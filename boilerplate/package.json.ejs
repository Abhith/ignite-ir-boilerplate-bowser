{
  "name": "ignite",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "compile": "tsc --noEmit -p .",
    "format": "npm-run-all format:*",
    "format:js": "prettier --write {.,**}/*.js",
    "format:json": "prettier --write {.,**}/*.json",
    "format:md": "prettier --write {.,**}/*.md",
    "format:ts": "prettier --write {.,**}/*.{ts,tsx} && tslint --fix -p .",
    "lint": "npm-run-all lint:*",
    "lint:ts": "tslint -p .",
    "postinstall": "solidarity",
    "precommit": "lint-staged",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "storybook": "storybook start -p 9001 -c .storybook --skip-packager",
    "test": "jest"
  },
  "dependencies": {
    "apisauce": "^0.14.3",
    "lodash.throttle": "^4.1.1",
    "mobx": "^3.4.1",
    "mobx-react": "^4.3.5",
    "mobx-state-tree": "^1.3.1",
    "ramda": "^0.25.0",
    "react-native-i18n": "^2.0.10",
    "react-native-keychain": "2.0.0-rc",
    "react-navigation": "^1.0.0-beta.22",
    "validate.js": "^0.12.0"
  },
  "devDependencies": {
    "@storybook/addon-storyshots": "^3.3.3",
    "@storybook/react-native": "^3.3.3",
    "@types/jest": "^22.0.0",
    "@types/ramda": "^0.25.13",
    "@types/react": "^16.0.33",
    "@types/react-native": "^0.51.8",
    "@types/react-navigation": "^1.0.26",
    "@types/react-test-renderer": "^16.0.0",
    "@types/validate.js": "0.0.3",
    "babel-jest": "^22.0.4",
    "babel-plugin-transform-inline-environment-variables": "^0.2.0",
    "babel-preset-react-native": "4.0.0",
    "husky": "^0.14.3",
    "jest": "^22.0.4",
    "jest-preset-typescript-react-native": "^1.2.0",
    "lint-staged": "^6.0.0",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.9.2",
    "react-dom": "^16.0.0",
    "react-native-typescript-transformer": "^1.1.4",
    "react-powerplug": "^0.1.2",
    "react-test-renderer": "^16.0.0",
    "reactotron-mst": "^1.15.0",
    "reactotron-react-native": "^1.14.0",
    "solidarity": "^1.1.0",
    "tslint": "^5.8.0",
    "tslint-config-prettier": "^1.6.0",
    "typescript": "^2.6.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "preset": "jest-preset-typescript-react-native",
    "setupFiles": [
      "<rootDir>/node_modules/react-native/jest/setup.js",
      "./test/setup.ts"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|react-navigation)"
    ]
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ],
    "*.{ts,tsx}": [
      "prettier --write",
      "tslint --fix",
      "git add"
    ],
    "*.md": [
      "prettier --write",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  }
}
